<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyHomeServlet</servlet-name>
  	<display-name>MyHomeServlet</display-name>
  	<description></description>
  	<servlet-class>com.globallogic.servlets.MyHomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MyLoginServlet</servlet-name>
  	<display-name>MyLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.globallogic.servlets.MyLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyHomeServlet</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MyLoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
